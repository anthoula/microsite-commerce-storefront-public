{
  "extends": "./config/tsconfig-astro.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@images/*": ["./src/content/docs/images/*"],
      "@components/*": ["./src/components/*"],
      "@data/*": ["./src/data/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
  },
}
