{
  "extends": "./config/tsconfig-astro.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@data/*": ["./src/data/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@images/*": ["./src/assets/images/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noImplicitAny": false
  }
}
