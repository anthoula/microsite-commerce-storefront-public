    "@types/hast": "^3.0.3",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "astro": "^4.0.9",
    "hast-util-from-html": "^2.0.1",
    "hast-util-to-string": "^3.0.0",
    "hastscript": "^8.0.0",
    "node-html-parser": "^6.1.12",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "prop-types": "^15.8.1",
