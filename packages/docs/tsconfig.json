{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@data/*": ["./src/data/*"],
      "@dropins/*": ["./src/content/docs/dropins/*"],
      "@sdk/*": ["src/content/docs/sdk/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
